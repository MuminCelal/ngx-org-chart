{
  "compileOnSave": false,
  "compilerOptions": {
    "incremental": true,
    "outDir": "./build",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES2015",
    "module": "ES2020",
    "lib": [
      "ES2020",
      "DOM"
    ],
    "baseUrl": "./",
    "paths": {
      "ngx-org-chart": [
        "build/ngx-org-chart/ngx-org-chart",
        "build/ngx-org-chart"
      ]
    }
  }
}
